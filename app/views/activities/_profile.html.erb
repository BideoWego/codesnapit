<div class="panel panel-default">
  <div class="panel-heading clearfix">
    <h3>
      <%= profile.user == current_user ? 'You' : link_to(profile.user.username, user_profile_path(profile.user)) %>
      <small>
        updated their profile on
        <%= profile.date(:method => :updated_at) %>
      </small>
    </h3>
  </div>
</div>





